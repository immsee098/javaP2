package com.day8;

import java.util.Scanner;

public class MethodPrac4_3_27 {
	
	public static int yearVer(int year) {
		boolean call = false;
		if(year %400==0 || (year%4 == 0 && year%100 != 0){
			call = true;
		}
		return call;
	}

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

	}

}
