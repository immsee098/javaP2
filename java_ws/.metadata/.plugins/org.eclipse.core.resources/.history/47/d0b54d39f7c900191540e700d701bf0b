package com.day8;

public class MethodTest {

	public static void main(String[] args) {
		for(int i=0; i<5; i++) {
			System.out.println("*");
		}
		
		System.out.println();
		
		System.out.println("java");
		
		
		for(int i=0; i<5; i++) {
			System.out.println("*");
		}
		
		System.out.println();
		
		System.out.println("hello");

		
		for(int i=0; i<5; i++) {
			System.out.println("*");
		}
		
		System.out.println();
		
		System.out.println("!");
	}

}
