package com.day8;

import java.util.Scanner;

public class MethodPrac2_3_25 {

	public static int numCalc(int a, int b) {
		while(true) {
			int result = 0;
			if(a>=b) {
				result = a%b;
			} else {
				result = b%a;
			}
			return result;
		}
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		while(true) {
			System.out.println("정수를 입력하세요");
			int x = sc.nextInt();
			int y = sc.nextInt();
			
			if(x==0 || y==0) {
				break;
			} else {
				System.out.println(numCalc(x, y));
			}
		}

	}

}
