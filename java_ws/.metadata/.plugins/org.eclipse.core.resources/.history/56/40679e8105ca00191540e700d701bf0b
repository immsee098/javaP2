package com.day8;

public class MethodTest3_3_17 {
	public static void func() {
		System.out.println("***********");
		//매개변수는 있고, 반환값은 없는 메서드 정의
		//입력받은 개수만큼 별을 출력하는 메서드
	}
	
	public static void func2(int cnt) {
		for(int i=0; i<cnt; i++) {
			System.out.print("*");
		}
		System.out.println();
	}
	public static void main(String[] args) {
		//[1]매개변수도 없고 반환값도 없는 메서드 호출
		func();
	}

}
