package com.day8;

public class MethodPrac3_3_26 {

	public static float toCm(float a) {
		float cm = (float)(2.54*a);
		return cm;
	}
	
	public static float toInch(float b) {
		float inch = (float) (b/2.54);
		return inch;
	}
