package com.day7;

import java.util.Scanner;

public class Prac1p120 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		int sum = 0;
		if(a>=b) {
			for(int i =b; i<a+1; i++) {
				sum += i;
			} 
		}else {
			for(int i =a; i<b+1; i++) {
			sum += i;
			}
			System.out.println("정수 사이의 합 : " + sum);
		}
	}

}
