package com.day5;

public class prac1 {

	public static void main(String[] args) {
		int i =200;
		char ch = ' ';
		
		if(i>100&&i<300) {
			System.out.println(true);
		}
		
		if(ch != ' ' || ch != '\t') {
			System.out.println('a'+ true);
		}
		
		if(ch=='x' || ch=='X') {
			System.out.println(true);
		}
		
		

	}

}
