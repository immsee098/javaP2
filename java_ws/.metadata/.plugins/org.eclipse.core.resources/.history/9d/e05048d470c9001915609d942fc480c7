package com.day7;

public class Prac2p121 {

	public static void main(String[] args) {
		for (int i =2; i<9; i+=2) {
			for(int j = 1; j< i+1; j++) {
				System.out.println(i+"*"+j+"="+i*j);
			}
			System.out.println();
		}
		
		System.out.println("======");
		
		for(int i=1; i<5; i++) {
			for(int j=(5*i)+1; j <(5*(i+1))+1; j++) {
				System.out.print(j + "\t");
			}
			System.out.println();
		}

	}

}
