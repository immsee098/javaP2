package com.day8;

public class MethodTest {

	public static void main(String[] args) {
		for(int i=0; i<5; i++) {
			System.out.print("*");
		}
		
		System.out.println();
		
		System.out.print("java");
		
		
		for(int i=0; i<5; i++) {
			System.out.print("*");
		}
		
		System.out.println();
		
		System.out.print("hello");

		
		for(int i=0; i<5; i++) {
			System.out.print("*");
		}
		
		System.out.println();
		
		System.out.print("!!!!!!");
		
		for(int i=0; i<5; i++) {
			System.out.print("*");
		}
	}

}
