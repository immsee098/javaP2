package com.day8;

import java.util.Scanner;

public class MethodTest {

	public static void main(String[] args) {
		x();
		
		System.out.print("java");
		System.out.println();
		
		
		x();
		
		System.out.print("hello");
		
		System.out.println();

		
		x();
		
		System.out.print("!!!!!!");
		System.out.println();
		
		x();
		
	}//main

	public static void x() {
		for(int i=0; i<5; i++) {
			System.out.print("*");
		}
		
		System.out.println();
		
	}
	
}//class
