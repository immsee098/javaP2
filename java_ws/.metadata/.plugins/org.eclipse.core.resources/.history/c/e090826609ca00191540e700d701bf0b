package com.day8;

public class MethodPrac1_3_22 {
	//[1]
	public static void func() {
		System.out.println("Hello java");
	}
	
	//[2]
	public static void func2(int a) {
		for(int i=0; i<a; i++) {
			System.out.println("Hello java");
		}
	}
	public static void main(String[] args) {
		func();
		System.out.println();
		func2(3);
		System.out.println();
	}

}
