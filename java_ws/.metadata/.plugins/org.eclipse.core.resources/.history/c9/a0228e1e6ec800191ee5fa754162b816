package com.day6;

import java.util.Scanner;

public class ForPrac1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int sum = 0;
		
		for(int i=0; i<n; i++) {
			System.out.println("재미있는 java!");
			System.out.println("i="+i);
		}

	}

}
