package com.day6;

public class ForPrac4 {

	public static void main(String[] args) {
		String sum = "";
		for (int i=1; i<100; i++) {
			if(i%7==0 && i%9==0) {
				sum = sum + (i + "  ");
				System.out.print(i + "  ");
			} else if (i%7==0) {
				if(i%3 == 0) {
					System.out.println(i + "  ");
				} else {
					System.out.print(i + "  ");
				}
				//sum = sum + (i + "  ");
			} else if (i%9==0) {
				System.out.print(i + "  ");
				sum = sum + (i + "  ");
			} 
			
		}

	}

}
