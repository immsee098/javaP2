package com.day8;

import java.util.Scanner;

public class MethodTest {

	public static void main(String[] args) {
		x();
		
		System.out.print("java");
		System.out.println();
		
		
		x();
		
		System.out.print("hello");
		
		System.out.println();

		
		x();
		
		System.out.print("!!!!!!");
		System.out.println();
		
		x();
		
		for(int i = 2; i<10; i+=2) {
			for(int j=1; j<i+1; j++) {
				System.out.println(i + "*" + j + "=" + i*j);
			}
		}


		
	}//main

	public static void x() {
		for(int i=0; i<5; i++) {
			System.out.print("*");
		}
		
		System.out.println();
		
	}
	
}//class
